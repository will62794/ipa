// Action Interaction Graph
digraph {
	rankdir=LR
	RequestVote [shape=box style=rounded]
	UpdateTerm [shape=box style=rounded]
	BecomeLeader [shape=box style=rounded]
	ClientRequest [shape=box style=rounded]
	AppendEntries [shape=box style=rounded]
	HandleRequestVoteRequest [shape=box style=rounded]
	HandleRequestVoteResponse [shape=box style=rounded]
	AcceptAppendEntriesRequestAppend [shape=box style=rounded]
	HandleAppendEntriesResponse [shape=box style=rounded]
	ClientRequest -> RequestVote [label=log fontsize=12]
	AcceptAppendEntriesRequestAppend -> RequestVote [label=log fontsize=12]
	RequestVote -> UpdateTerm [label=requestVoteRequestMsgs fontsize=12]
	AppendEntries -> UpdateTerm [label=appendEntriesRequestMsgs fontsize=12]
	HandleRequestVoteRequest -> UpdateTerm [label="requestVoteResponseMsgs,requestVoteRequestMsgs" fontsize=12]
	HandleRequestVoteResponse -> UpdateTerm [label=requestVoteResponseMsgs fontsize=12]
	AcceptAppendEntriesRequestAppend -> UpdateTerm [label=appendEntriesResponseMsgs fontsize=12]
	HandleAppendEntriesResponse -> UpdateTerm [label=appendEntriesResponseMsgs fontsize=12]
	RequestVote -> BecomeLeader [label=votesGranted fontsize=12]
	ClientRequest -> BecomeLeader [label=log fontsize=12]
	HandleRequestVoteResponse -> BecomeLeader [label=votesGranted fontsize=12]
	AcceptAppendEntriesRequestAppend -> BecomeLeader [label=log fontsize=12]
	RequestVote -> ClientRequest [label="currentTerm,state" fontsize=12]
	UpdateTerm -> ClientRequest [label="currentTerm,state" fontsize=12]
	BecomeLeader -> ClientRequest [label=state fontsize=12]
	RequestVote -> AppendEntries [label="currentTerm,state" fontsize=12]
	UpdateTerm -> AppendEntries [label="currentTerm,state" fontsize=12]
	BecomeLeader -> AppendEntries [label="state,nextIndex" fontsize=12]
	ClientRequest -> AppendEntries [label=log fontsize=12]
	AcceptAppendEntriesRequestAppend -> AppendEntries [label=log fontsize=12]
	HandleAppendEntriesResponse -> AppendEntries [label=nextIndex fontsize=12]
	RequestVote -> HandleRequestVoteRequest [label=currentTerm fontsize=12]
	UpdateTerm -> HandleRequestVoteRequest [label=currentTerm fontsize=12]
	ClientRequest -> HandleRequestVoteRequest [label=log fontsize=12]
	AcceptAppendEntriesRequestAppend -> HandleRequestVoteRequest [label=log fontsize=12]
	RequestVote -> HandleRequestVoteResponse [label=currentTerm fontsize=12]
	UpdateTerm -> HandleRequestVoteResponse [label=currentTerm fontsize=12]
	RequestVote -> AcceptAppendEntriesRequestAppend [label="currentTerm,state" fontsize=12]
	UpdateTerm -> AcceptAppendEntriesRequestAppend [label="currentTerm,state" fontsize=12]
	BecomeLeader -> AcceptAppendEntriesRequestAppend [label=state fontsize=12]
	AppendEntries -> AcceptAppendEntriesRequestAppend [label=appendEntriesRequestMsgs fontsize=12]
	RequestVote -> HandleAppendEntriesResponse [label=currentTerm fontsize=12]
	UpdateTerm -> HandleAppendEntriesResponse [label=currentTerm fontsize=12]
}
