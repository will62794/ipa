// Action Interaction Graph
digraph {
	rankdir=LR
	ClientRequest [shape=box style=rounded]
	GetEntries [shape=box style=rounded]
	RollbackEntries [shape=box style=rounded]
	BecomeLeader [shape=box style=rounded]
	CommitEntry [shape=box style=rounded]
	UpdateTerms [shape=box style=rounded]
	Reconfig [shape=box style=rounded]
	SendConfig [shape=box style=rounded]
	GetEntries -> ClientRequest [label=log fontsize=12]
	RollbackEntries -> ClientRequest [label=log fontsize=12]
	BecomeLeader -> ClientRequest [label="state,currentTerm" fontsize=12]
	UpdateTerms -> ClientRequest [label="state,currentTerm" fontsize=12]
	ClientRequest -> GetEntries [label=log fontsize=12]
	RollbackEntries -> GetEntries [label=log fontsize=12]
	BecomeLeader -> GetEntries [label=state fontsize=12]
	UpdateTerms -> GetEntries [label=state fontsize=12]
	ClientRequest -> RollbackEntries [label=log fontsize=12]
	GetEntries -> RollbackEntries [label=log fontsize=12]
	BecomeLeader -> RollbackEntries [label=state fontsize=12]
	UpdateTerms -> RollbackEntries [label=state fontsize=12]
	ClientRequest -> BecomeLeader [label=log fontsize=12]
	GetEntries -> BecomeLeader [label=log fontsize=12]
	RollbackEntries -> BecomeLeader [label=log fontsize=12]
	UpdateTerms -> BecomeLeader [label="state,currentTerm" fontsize=12]
	Reconfig -> BecomeLeader [label="config,configTerm,configVersion" fontsize=12]
	SendConfig -> BecomeLeader [label="config,configTerm,configVersion" fontsize=12]
	ClientRequest -> CommitEntry [label=log fontsize=12]
	GetEntries -> CommitEntry [label=log fontsize=12]
	RollbackEntries -> CommitEntry [label=log fontsize=12]
	BecomeLeader -> CommitEntry [label="state,currentTerm" fontsize=12]
	UpdateTerms -> CommitEntry [label="state,currentTerm" fontsize=12]
	Reconfig -> CommitEntry [label=config fontsize=12]
	SendConfig -> CommitEntry [label=config fontsize=12]
	BecomeLeader -> UpdateTerms [label="state,currentTerm" fontsize=12]
	ClientRequest -> Reconfig [label=log fontsize=12]
	GetEntries -> Reconfig [label=log fontsize=12]
	RollbackEntries -> Reconfig [label=log fontsize=12]
	BecomeLeader -> Reconfig [label="state,configTerm,currentTerm" fontsize=12]
	CommitEntry -> Reconfig [label=immediatelyCommitted fontsize=12]
	UpdateTerms -> Reconfig [label="state,currentTerm" fontsize=12]
	SendConfig -> Reconfig [label="config,configTerm,configVersion" fontsize=12]
	BecomeLeader -> SendConfig [label="state,configTerm" fontsize=12]
	UpdateTerms -> SendConfig [label=state fontsize=12]
	Reconfig -> SendConfig [label="config,configTerm,configVersion" fontsize=12]
	dpi=300
}
