	TMAbort -> TMRcvPrepared [label=tmState fontsize=8]
	TMCommit -> TMRcvPrepared [label=tmState fontsize=8]
	RMChooseToAbort -> RMPrepare [label=rmState fontsize=8]
	RMRcvCommitMsg -> RMPrepare [label=rmState fontsize=8]
	RMRcvAbortMsg -> RMPrepare [label=rmState fontsize=8]
	RMPrepare -> RMChooseToAbort [label=rmState fontsize=8]
	RMRcvCommitMsg -> RMChooseToAbort [label=rmState fontsize=8]
	RMRcvAbortMsg -> RMChooseToAbort [label=rmState fontsize=8]
	RMPrepare -> RMRcvCommitMsg [label=rmState fontsize=8]
	RMChooseToAbort -> RMRcvCommitMsg [label=rmState fontsize=8]
	RMRcvAbortMsg -> RMRcvCommitMsg [label=rmState fontsize=8]
	TMCommit -> RMRcvCommitMsg [label=msgsCommit fontsize=8]
	RMPrepare -> RMRcvAbortMsg [label=rmState fontsize=8]
	RMChooseToAbort -> RMRcvAbortMsg [label=rmState fontsize=8]
	RMRcvCommitMsg -> RMRcvAbortMsg [label=rmState fontsize=8]
	TMAbort -> RMRcvAbortMsg [label=msgsAbort fontsize=8]
	TMCommit -> TMAbort [label=tmState fontsize=8]
	TMRcvPrepared -> TMCommit [label=tmPrepared fontsize=8]
	TMAbort -> TMCommit [label=tmState fontsize=8]
}
