// Action Interaction Graph
digraph {
	rankdir=LR
	Request [shape=box style=rounded]
	Enter [shape=box style=rounded]
	Exit [shape=box style=rounded]
	ReceiveRequest [shape=box style=rounded]
	ReceiveAck [shape=box style=rounded]
	ReceiveRelease [shape=box style=rounded]
	Exit -> Request [label="network,ack,req" fontsize=12]
	ReceiveRequest -> Request [label="clock,req,network" fontsize=12]
	ReceiveAck -> Request [label="ack,network" fontsize=12]
	ReceiveRelease -> Request [label="req,network" fontsize=12]
	Request -> Enter [label="ack,req" fontsize=12]
	Exit -> Enter [label="ack,crit,req" fontsize=12]
	ReceiveRequest -> Enter [label=req fontsize=12]
	ReceiveAck -> Enter [label=ack fontsize=12]
	ReceiveRelease -> Enter [label=req fontsize=12]
	Request -> Exit [label="ack,req,network" fontsize=12]
	Enter -> Exit [label=crit fontsize=12]
	ReceiveRequest -> Exit [label="req,network" fontsize=12]
	ReceiveAck -> Exit [label="ack,network" fontsize=12]
	ReceiveRelease -> Exit [label="req,network" fontsize=12]
	Request -> ReceiveRequest [label="req,network" fontsize=12]
	Exit -> ReceiveRequest [label="network,req" fontsize=12]
	ReceiveAck -> ReceiveRequest [label=network fontsize=12]
	ReceiveRelease -> ReceiveRequest [label="req,network" fontsize=12]
	Request -> ReceiveAck [label="ack,network" fontsize=12]
	Exit -> ReceiveAck [label="ack,network" fontsize=12]
	ReceiveRequest -> ReceiveAck [label=network fontsize=12]
	ReceiveRelease -> ReceiveAck [label=network fontsize=12]
	Request -> ReceiveRelease [label="network,req" fontsize=12]
	Exit -> ReceiveRelease [label="network,req" fontsize=12]
	ReceiveRequest -> ReceiveRelease [label="network,req" fontsize=12]
	ReceiveAck -> ReceiveRelease [label=network fontsize=12]
	dpi=300
}
