	RMRcvAbortMsg -> RMRcvCommitMsg [label=rmState fontsize=12]
	TMAbort -> RMRcvCommitMsg [label=msgs fontsize=12]
	TMCommit -> RMRcvCommitMsg [label=msgs fontsize=12]
	RMPrepare -> RMRcvAbortMsg [label="rmState,msgs" fontsize=12]
	RMChooseToAbort -> RMRcvAbortMsg [label=rmState fontsize=12]
	RMRcvCommitMsg -> RMRcvAbortMsg [label=rmState fontsize=12]
	TMAbort -> RMRcvAbortMsg [label=msgs fontsize=12]
	TMCommit -> RMRcvAbortMsg [label=msgs fontsize=12]
	RMPrepare -> TMAbort [label=msgs fontsize=12]
